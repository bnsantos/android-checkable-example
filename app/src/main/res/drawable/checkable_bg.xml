<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <item android:drawable="@android:color/white" />
  <item android:width="24dp" android:height="24dp" android:bottom="500dp" android:right="500dp" >
    <selector>
      <item app:selectable="true" android:state_checked="true" android:drawable="@drawable/ic_checked" />
      <item  app:selectable="true" android:state_checked="false" android:drawable="@drawable/ic_unchecked" />
    </selector>
  </item>
</layer-list>
